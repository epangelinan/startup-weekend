<!--index.vtl-->
<h1>Member list!</h1>
#if ($members)
  <ul>
    #foreach ($member in $members)
      <li>$member.getName()</li>
    #end
  </ul>
#else
  <p>No members!</p>
#end

<p>Add a new Member:</p>

<form action="/members" method="post">
  <label for="memberName">Member Name</label>
  <input id="memberName" name="memberName" type="text">

  <button type="submit" class="btn btn-default">Add member</button>
</form>
<!--index.vtl-->
